{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "target": "es6",
    "typeRoots": [
      "node_modules/@types",
      "src/customInclude"
    ],
    "lib": [
      "es2021",
      "dom"
    ]
  },
  "exclude": [
  ]
}
